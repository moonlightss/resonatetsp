<p id="notice"><%= notice %></p>

<h1>Listing Dses</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @dses.each do |dse| %>
      <tr>
        <td><%= dse.name %></td>
        <td><%= link_to 'Show', dse %></td>
        <td><%= link_to 'Edit', edit_dse_path(dse) %></td>
        <td><%= link_to 'Destroy', dse, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Dse', new_dse_path %>
